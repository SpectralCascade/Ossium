<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{soup_app_name}" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="release">
				<Option output="bin/release/{soup_app_name}" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/release/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-std=c++1z" />
					<Add option="-g" />
					{soup_include_paths}
				</Compiler>
				<Linker>
					<Add option="-static-libstdc++" />
					<Add option="-static-libgcc" />
                    <Add option="-lmingw32" />
                    <Add option="-lSDL2main" />
					<Add option="-lSDL2" />
                    <Add option="-lSDL2_image" />
					<Add option="-lSDL2_mixer" />
					<Add option="-lSDL2_ttf" />
                    <Add option="-lSDL2_net" />
                    <Add option="-lbgfxRelease" />
                    <Add option="-lbimgRelease" />
                    <Add option="-lbxRelease" />
                    <Add option="-lgdi32" />
					{soup_lib_paths}
					<Add option="-Wl,-Bstatic -lstdc++ -lpthread -Wl,-Bdynamic -static-libgcc -static-libstdc++" />
				</Linker>
			</Target>
			<Target title="ossiumdll">
				<Option output="bin/ossiumdll/ossium" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/ossiumdll/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-DOSSIUM_EXPORT_DLL" />
                    {soup_include_paths}
				</Compiler>
				<Linker>
					<Add option="-Wl,-Bstatic -lstdc++ -lpthread -Wl,-Bdynamic" />
                    <Add option="-lSDL2main" />
					<Add option="-lSDL2" />
                    <Add option="-lSDL2_image" />
					<Add option="-lSDL2_mixer" />
					<Add option="-lSDL2_ttf" />
                    <Add option="-lSDL2_net" />
                    <Add option="-lbgfxRelease" />
                    <Add option="-lbimgRelease" />
                    <Add option="-lbxRelease" />
                    <Add option="-lgdi32" />
                    {soup_lib_paths}
				</Linker>
			</Target>
			<Target title="debug_editor">
				<Option output="bin/debug_editor/{soup_app_name}Editor" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/debug_editor/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-std=c++1z" />
					<Add option="-g" />
					<Add option="-DOSSIUM_DEBUG" />
					<Add option="-DOSSIUM_EDITOR" />
                    {soup_include_paths}
				</Compiler>
				<Linker>
					<Add option="-static-libstdc++" />
					<Add option="-static-libgcc" />
					<Add option="-lmingw32" />
					<Add option="-lcomdlg32" />
					<Add option="-lole32" />
					<Add option="-lSDL2main" />
					<Add option="-lSDL2" />
                    <Add option="-lSDL2_image" />
					<Add option="-lSDL2_mixer" />
					<Add option="-lSDL2_ttf" />
                    <Add option="-lSDL2_net" />
                    <Add option="-lbgfxRelease" />
                    <Add option="-lbimgRelease" />
                    <Add option="-lbxRelease" />
                    <Add option="-lgdi32" />
					{soup_lib_paths}
					<Add option="-Wl,-Bstatic -lstdc++ -lpthread -Wl,-Bdynamic -static-libgcc -static-libstdc++" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++1z" />
			<Add directory="Box2D" />
		</Compiler>
		<Linker>
			<Add directory="Box2D/bin/Release" />
		</Linker>
		{soup_header_paths}
        {soup_source_paths}
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
